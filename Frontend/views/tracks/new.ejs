<form action="/tracks"  method="post" enctype="multipart/form-data">  
<div class="wow bounceInUp formulario">
  <label  class="upload_field"><b>Upload a new track:</b></label><br><br>
  <label for="nombre" class="upload_field">Name:</label><br>
  <input type="text" name="nombre" placeholder="Name"><br><br>
    <label for="caratula" class="upload_field">File:</label><br>
  <input class="upload_field" type="file" accept=".mp3,.wav,.ogg" name="track"/><br><br>
  <label for="caratula" class="upload_field">Cover:</label><br>
  <input class="upload_field" type="file" accept="image/*" name="caratula"/><br><br>


  <button  type="submit" class="btn btn-1 btn-1d">Upload</button><br><br>
  <span class="hint upload_field">Supported files: MP3, Wav, and Ogg</span>
  </div>
</form>